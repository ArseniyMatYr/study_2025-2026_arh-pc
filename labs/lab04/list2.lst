     1                                  ; lab4.asm
     2                                  SECTION .data ; Начало секции данных
     3 00000000 4D617469656E6B6F20-     	hello: DB 'Matienko Arsenij!',10 ; 'Hello world!' плюс
     3 00000009 417273656E696A210A 
     4                                  				; символ перевода строки
     5                                  	helloLen: EQU $-hello 	; Длина строки hello
     6                                  SECTION .text 		; Начало секции кода
     7                                  	GLOBAL _start
     8                                  	
     9                                  _start: 		; Точка входа в программу
    10 00000000 B804000000              	mov eax,4 	; Системный вызов для записи (sys_write)
    11 00000005 BB01000000              	mov ebx,1 	; Описатель файла '1' - стандартный вывод
    12 0000000A B9[00000000]            	mov ecx,hello 	; Адрес строки hello в ecx
    13 0000000F BA12000000              	mov edx,helloLen ; Размер строки hello
    14 00000014 CD80                    	int 80h 	; Вызов ядра
    15 00000016 B801000000              	mov eax,1 	; Системный вызов для выхода (sys_exit)
    16 0000001B BB00000000              	mov ebx,0 	; Выход с кодом возврата '0' (без ошибок)
    17 00000020 CD80                    	int 80h 	; Вызов ядра
